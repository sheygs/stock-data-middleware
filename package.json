{
        "name": "stock-data-middleware",
        "version": "1.0.0",
        "description": "A minimalistic implementation of the stock market  API",
        "main": "index.js",
        "scripts": {
                "test": "jest --watch --detectOpenHandles --verbose",
                "clean": "rimraf dist",
                "build": "npm run clean && babel ./src --out-dir ./dist",
                "start": "npm run build && nodemon --exec babel-node src/index.js",
                "migrate:db": "babel-node -- src/migrations/table.js",
                "prod-migrate:db": "node dist/migrations/table.js",
                "serve": "node dist/index.js"
        },
        "repository": {
                "type": "git",
                "url": "git+https://github.com/sheygs13/stock-data-middleware.git"
        },
        "keywords": [
                "stock",
                "market",
                "stock market"
        ],
        "author": "Olusegun Ekoh (Sheygs)",
        "license": "MIT",
        "bugs": {
                "url": "https://github.com/sheygs13/stock-data-middleware/issues"
        },
        "homepage": "https://github.com/sheygs13/stock-data-middleware#readme",
        "devDependencies": {
                "@babel/cli": "^7.16.0",
                "@babel/core": "^7.16.0",
                "@babel/node": "^7.16.0",
                "@babel/polyfill": "^7.12.1",
                "@babel/preset-env": "^7.16.4",
                "@babel/runtime": "^7.16.3",
                "jest": "^27.4.3",
                "nock": "^13.2.1",
                "nodemon": "^2.0.15",
                "rimraf": "^3.0.2",
                "supertest": "^6.1.6"
        },
        "dependencies": {
                "axios": "^0.24.0",
                "cors": "^2.8.5",
                "debug": "^4.3.3",
                "dotenv": "^10.0.0",
                "express": "^4.17.1",
                "helmet": "^4.6.0",
                "morgan": "^1.10.0",
                "node-cron": "^3.0.0",
                "pg": "^8.7.1",
                "swagger-ui-express": "^4.2.0",
                "winston": "^3.3.3"
        }
}
